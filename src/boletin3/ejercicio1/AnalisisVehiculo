class Vehiculo
	- String marca
	- String modelo
	- String color
	- String matricula
	--------------------------
	- boolean encendido
	- int marcha
	- int velocidad
	--------------------------
	Vehiculo(String marca, String modelo, String color, String matricula)
	--------------------------
	+ getMarca()
	+ getModelo()
	+ getColor()
	+ getMatricula()
	+ getEncendido()
	+ getMarcha()
	+ getVelocidad()
	--------------------------
	+ setColor(String color)
	+ setEncendido(boolean encendido)
	+ setMarcha(int marcha)
	+ setVelocidad(int velocidad)
	--------------------------
	+ void parar()
	+ void arrancar()
	+ void subirMarcha()
	+ void bajarMarcha()
	+ boolean acelerar(int velocidad)
	+ boolean desacelerar(int velocidad)
	--------------------------
	+ String toString()
	+ boolean equals()
	
class Turismo extends Vehiculo
	enum TipoUso{PROFESIONAL, PARTICULAR}
	--------------------------
	- int plazas
	- TipoUso uso
	--------------------------
	Turismo(String marca, String modelo, String color, String matricula, int plazas, String uso)
	--------------------------
	+ getPlazas()
	+ getUso()
	--------------------------
	+ setUso(String uso)
	
class Camion extends Vehiculo
	- double pesoMaximo
	- boolean mercanciaPeligrosa
	--------------------------
	Camion(String marca, String modelo, String color, String matricula, double pesoMaximo, boolean mercanciaPeligrosa)
	--------------------------
	+ getPesoMaximo()
	+ getMercanciaPeligrosa()
	--------------------------
	+ setMercanciaPeligrosa(boolean mercancia)
	
class Motocicleta extends Vehiculo
	- int cilindrada
	--------------------------
	- boolean carnet
	--------------------------
	Motocicleta (String marca, String modelo, String color, String matricula, int cilindrada)
	--------------------------
	+ getCilindrada()
	+ isRequiereCarnet()
	--------------------------
	+ boolean requiereCarnet()